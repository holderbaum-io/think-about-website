<div>
  <h1 id="partners"><%= t('partners.title') %></h1>
  <% data('partners').reject { |p| p[:hidden] }.each do |partner| %>
    <section>
      <a href="<%= partner[:link] %>">
        <figure>
          <img style="height: <%= partner.fetch(:height, 50) %>px" src="/assets/images/partner/<%= partner[:name] %>.png"/>
        </figure>
      </a>
    </section>
  <% end %>
</div>
